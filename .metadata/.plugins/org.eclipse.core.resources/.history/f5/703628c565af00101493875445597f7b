package test_util;

import static org.junit.Assert.*;

import org.junit.Before;
import org.junit.Test;

import junit.framework.Assert;
import util.Mensajes;

public class Test_Mensajes {

    @Before
    public void setUp() throws Exception {
    
    }

    @Test
    public void check_USUARIO_REPETIDO() {
        String USUARIO_REPETIDO = "Usuario repetido";
        Mensajes msj = Mensajes.USUARIO_REPETIDO;
        Assert.assertEquals(msj.getValor(), USUARIO_REPETIDO);
    }

    @Test
    public void check_PASS_ERRONEO() {
        String PASS_ERRONEO = "Password incorrecto";
        Mensajes msj = Mensajes.PASS_ERRONEO;
        Assert.assertEquals(msj.getValor(), PASS_ERRONEO);
    }

    @Test
    public void check_USUARIO_DESCONOCIDO() {
        String USUARIO_DESCONOCIDO = "Usuario inexistente";
        Mensajes msj = Mensajes.USUARIO_DESCONOCIDO;
        Assert.assertEquals(msj.getValor(), USUARIO_DESCONOCIDO);
    }

    @Test
    public void check_PARAMETROS_NULOS() {
        String PARAMETROS_NULOS = "Algun parametro requerido es null";
        Mensajes msj = Mensajes.PARAMETROS_NULOS;
        Assert.assertEquals(msj.getValor(), PARAMETROS_NULOS);
    }

    @Test
    public void check_PASS_NO_COINCIDE() {
        String PASS_NO_COINCIDE = "La contrasena y su confirmacion no coinciden";
        Mensajes msj = Mensajes.PASS_NO_COINCIDE;
        System.out.println(msj.getValor());
        Assert.assertEquals(msj.getValor(), PASS_NO_COINCIDE);
    }

    @Test
    public void check_SIN_VEHICULO_PARA_PEDIDO() {
        String SIN_VEHICULO_PARA_PEDIDO = "Ningun vehiculo puede satisfacer el pedido";
        Mensajes msj = Mensajes.SIN_VEHICULO_PARA_PEDIDO;
        Assert.assertEquals(msj.getValor(), SIN_VEHICULO_PARA_PEDIDO);
    }

    @Test
    public void check_PEDIDO_INEXISTENTE() {
        String PEDIDO_INEXISTENTE = "El Pedido no figura en la lista";
        Mensajes msj = Mensajes.PEDIDO_INEXISTENTE;
        Assert.assertEquals(msj.getValor(), PEDIDO_INEXISTENTE);
    }

    @Test
    public void check_CHOFER_NO_DISPONIBLE() {
        String CHOFER_NO_DISPONIBLE = "El chofer no esta disponoble";
        Mensajes msj = Mensajes.CHOFER_NO_DISPONIBLE;
        Assert.assertEquals(msj.getValor(), CHOFER_NO_DISPONIBLE);
    }

    @Test
    public void check_VEHICULO_NO_DISPONIBLE() {
        String VEHICULO_NO_DISPONIBLE = "El vehiculo no esta disponible";
        Mensajes msj = Mensajes.VEHICULO_NO_DISPONIBLE;
        Assert.assertEquals(msj.getValor(), VEHICULO_NO_DISPONIBLE);
    }

    @Test
    public void check_VEHICULO_NO_VALIDO() {
        String VEHICULO_NO_VALIDO = "El vehiculo no puede atender del pedido";
        Mensajes msj = Mensajes.VEHICULO_NO_VALIDO;
        Assert.assertEquals(msj.getValor(), VEHICULO_NO_VALIDO);
    }

    @Test
    public void check_VIAJE_INEXISTENTE() {
        String VIAJE_INEXISTENTE = "El viaje no esta regitrado";
        // Mensajes msj = Mensajes.VIAJE_INEXISTENTE; // No esta definido
        // Assert.assertEquals(msj.getValor(), VIAJE_INEXISTENTE);
        Assert.assertTrue(false);
    }

    @Test
    public void check_VIAJE_YA_FINALIZADO() {
        String VIAJE_YA_FINALIZADO = "El viaje ya figura como finalizado";
        // Mensajes msj = Mensajes.VIAJE_YA_FINALIZADO; // No esta definido
        // Assert.assertEquals(msj.getValor(), VIAJE_YA_FINALIZADO);
        Assert.assertTrue(false);
    }

    @Test
    public void check_CLIENTE_CON_VIAJE_PENDIENTE() {
        String CLIENTE_CON_VIAJE_PENDIENTE = "Cliente con viaje pendiente";
        Mensajes msj = Mensajes.CLIENTE_CON_VIAJE_PENDIENTE;
        Assert.assertEquals(msj.getValor(), CLIENTE_CON_VIAJE_PENDIENTE);
    }

    @Test
    public void check_VEHICULO_YA_REGISTRADO() {
        String VEHICULO_YA_REGISTRADO = "Vehiculo ya Registrado";
        Mensajes msj = Mensajes.VEHICULO_YA_REGISTRADO;
        Assert.assertEquals(msj.getValor(), VEHICULO_YA_REGISTRADO);
    }

    @Test
    public void check_CHOFER_YA_REGISTRADO() {
        String CHOFER_YA_REGISTRADO = "Chofer Ya Registrado";
        Mensajes msj = Mensajes.CHOFER_YA_REGISTRADO;
        Assert.assertEquals(msj.getValor(), CHOFER_YA_REGISTRADO);
    }

    @Test
    public void check_CHOFER_SIN_VIAJES() {
        String CHOFER_SIN_VIAJES = "El chofer no tiene ningun viaje realizado";
        Mensajes msj = Mensajes.CHOFER_SIN_VIAJES;
        Assert.assertEquals(msj.getValor(), CHOFER_SIN_VIAJES);
    }

}